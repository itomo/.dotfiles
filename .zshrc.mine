fpath=(${HOME}/.dotfiles/zsh-completions/src $fpath)
case ${OSTYPE} in
    darwin*)
        export PATH="$(brew --prefix coreutils)/libexec/gnubin:$PATH"
    ;;
esac
export LANG=ja_JP.UTF-8
setopt nonomatch
umask 002
cdpath=(~)
setopt AUTOPUSHD
setopt PUSHD_IGNORE_DUPS
setopt AUTO_CD
source ~/.zshrc.bindkey
#source ~/.zshrc.prompt
source ~/.zshrc.complete
source ~/.zshrc.history
source ~/.zshrc.alias
